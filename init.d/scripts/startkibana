# Copy Kibana logo image 

service_name='/apps/kibana/latest/bin/'

PID=$(ps -ef | grep $service_name | grep -v grep)
test=$?
if [ $test -eq 0 ]
then
  PID=$(ps -ef | grep $service_name | grep -v grep | tr -s ' ' | cut -d " " -f2)
  echo Kibana is Running as $PID
  #kill -9 $PID
else
  cd /apps/kibana
  echo "Started the Kibana.."
  #su - kibana -c "nohup /apps/kibana/latest/bin/kibana  &> /dev/null &"
  #nohup /apps/kibana/latest/bin/kibana &> /dev/null &
  nohup /apps/kibana/latest/bin/kibana > /dev/null 2>&1 &
  #/apps/kibana/latest/bin/kibana
  #sleep 5
  #PID=$(ps -ef | grep $service_name | grep -v grep | tr -s ' ' | cut -d " " -f2)
  #echo Kibana is running as $PID
fi
